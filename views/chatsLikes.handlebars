<body>
    <header>
        <h1>Tinder Dashboard</h1>
    </header>
    
    <main>
        <section class="likes-section">
            <h2>Profiles Who Liked You</h2>
            <div class="profiles">
                {{#each likes}}
                    <div class="profile-card">
                        <img src="{{image}}" alt="{{name}}'s profile picture">
                        <h3>{{name}}, {{age}}</h3>
                    </div>
                {{/each}}
            </div>
        </section>
        
        <section class="messages-section">
            <h2>Messages</h2>
            <div class="messages">
                {{#each messages}}
                    <div class="message-card">
                        <img src="{{image}}" alt="{{name}}'s profile picture">
                        <div class="message-info">
                            <h3>{{name}}, {{age}}</h3>
                            <p>{{lastMessage}}</p>
                        </div>
                    </div>
                {{/each}}
            </div>
        </section>
    </main>
</body>